, type=\"candlestick\",\n","                       open = ~usdt$Open, close = ~usdt$Close,\n","                       high = ~usdt$High, low = ~usdt$Low,\n","                       increasing = i, decreasing = d)\n","fig <- fig %>% layout(title = 'Prix du Tether en fonction du temps',\n","         yaxis = list(title = 'Prix ($)'))\n","fig"]},{"cell_type":"code","execution_count":null,"metadata":{"trusted":true},"outputs":[],"source":["usdc<-read.csv(\"C:\\Users\\enzoi\Documents\\Polytech\\ValorisationDesDonnes\\data\\coin_USDCoin.csv\")\n","usdc$Date <- as.Date(usdc$Date)\n","fig <- usdc %>% plot_ly(x = ~usdc$Date, type=\"candlestick\",\n","                       open = ~usdc$Open, close = ~usdc$Close,\n","                       high = ~usdc$High, low = ~usdc$Low,\n","                       increasing = i, decreasing = d)\n","fig <- fig %>% layout(title = 'Prix de l\\'USDCoin en fonction du temps',\n","         yaxis = list(title = 'Prix ($)'))\n","fig"]},{"cell_type":"markdown","metadata":{},"source":["# Traitement de la base de données et prédictions à l'aide des modèles ARIMA"]},{"cell_type":"markdown","metadata":{},"source":["Nous nous sommes rendu compte que l'étude des stable coins présentait peu d'intérêt, nous avons donc décidé de concentrer nos simulations sur les quatre cryptomonnaies restantes. Tout d'abord nous définissons les fonctions qui nous permettront de simuler les modèles obtenus."]},{"cell_type":"code","execution_count":null,"metadata":{"trusted":true},"outputs":[],"source":["date_to_vector <- function(date){\n","  y = format(as.Date(date, format=\"%Y-%m-%d\"),\"%Y\")\n","  y_int = as.double(y)\n","  n_days = difftime(as.Date(date),as.Date(paste(y,\"-01-01\",sep = \"\"),format=\"%Y-%m-%d\"),units = c(\"days\"))\n","  n_days_int = as.integer(n_days)\n","  return(c(y_int,n_days_int))\n","}\n","\n","forecast_arima <- function(data, model, h_pred) {\n","  #t1 = t2 = seq(as.Date(data$Date[1]),length=length(btc$Date),by=\"day\")\n","  #t2 = seq(as.Date(data$Date[1]),length=index,by=\"day\")\n","  s = date_to_vector(data$Date[1])\n","  data_ts = ts(data$Close, start = s)\n","  split <- ts_split(ts.obj = data_ts, sample.out = h_pred)\n","  \n","  testing <- split$test\n","  \n","  sim = simulate(model, nsim = h_pred)\n","  forecast1 = forecast(model, h = h_pred)\n","  df = ts_data.frame(forecast1$mean)\n","  df$true = testing\n","  df$max.80 = forecast1$upper[, 1]\n","  df$max.95 = forecast1$upper[, 2]\n","  df$min.80 = forecast1$lower[, 1]\n","  df$min.95 = forecast1$lower[, 2]\n","  df$sim = sim\n","  names(df)[names(df) == \"value\"] <- \"mean\"\n","  \n","  fig <-\n","    plot_ly(\n","      df,\n","      x = ~ time,\n","      y = ~ mean,\n","      name = 'mean',\n","      type = 'scatter',\n","      mode = 'lines',\n","      color = I(\"red\")\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ max.80,\n","      mode = 'lines',\n","      color = I(\"black\"),\n","      showlegend = FALSE\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ min.80,\n","      name = 'min-max 80%',\n","      mode = 'lines',\n","      fill = \"tonexty\",\n","      color = I(\"black\")\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ max.95,\n","      mode = 'lines',\n","      color = I(\"grey\"),\n","      showlegend = FALSE\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ min.95,\n","      name = 'min-max 95%',\n","      mode = 'lines',\n","      fill = \"tonexty\",\n","      color = I(\"grey\")\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ true,\n","      name = 'true values',\n","      mode = 'lines',\n","      color = I(\"orange\")\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ sim,\n","      name = 'Simulation',\n","      mode = 'lines',\n","      color = I(\"green\")\n","    )\n","  fig <-\n","    fig %>% layout(xaxis = list(type = \"date\", title = \"time\"),\n","                   yaxis = list(title = \"Price\"))\n","  return(list(fig, df, model))\n","  \n","}\n","\n","forecast_arima_auto <- function(data, index, h_pred) {\n","  #t1 = t2 = seq(as.Date(data$Date[1]),length=length(btc$Date),by=\"day\")\n","  #t2 = seq(as.Date(data$Date[1]),length=index,by=\"day\")\n","  s = date_to_vector(data$Date[1])\n","  data_ts = ts(data$Close,\n","               frequency = 365,\n","               start = s)\n","  data_ts2 = ts(data$Close[1:index],\n","                frequency = 360,\n","                start = s)\n","  model = auto.arima(\n","    data_ts2,\n","    method = \"ML\",\n","    approximation = TRUE,\n","    stepwise = TRUE,\n","    seasonal = TRUE\n","  )\n","  sim = simulate(model, nsim = h_pred)\n","  forecast1 = forecast(model, h = h_pred)\n","  df = ts_data.frame(forecast1$mean)\n","  df$true = data_ts[index:(index + h_pred - 1)]\n","  df$max.80 = forecast1$upper[, 1]\n","  df$max.95 = forecast1$upper[, 2]\n","  df$min.80 = forecast1$lower[, 1]\n","  df$min.95 = forecast1$lower[, 2]\n","  df$sim = sim\n","  names(df)[names(df) == \"value\"] <- \"mean\"\n","  \n","  fig <-\n","    plot_ly(\n","      df,\n","      x = ~ time,\n","      y = ~ mean,\n","      name = 'mean',\n","      type = 'scatter',\n","      mode = 'lines',\n","      color = I(\"red\")\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ max.80,\n","      mode = 'lines',\n","      color = I(\"black\"),\n","      showlegend = FALSE\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ min.80,\n","      name = 'min-max 80%',\n","      mode = 'lines',\n","      fill = \"tonexty\",\n","      color = I(\"black\")\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ max.95,\n","      mode = 'lines',\n","      color = I(\"grey\"),\n","      showlegend = FALSE\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ min.95,\n","      name = 'min-max 95%',\n","      mode = 'lines',\n","      fill = \"tonexty\",\n","      color = I(\"grey\")\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ true,\n","      name = 'true values',\n","      mode = 'lines',\n","      color = I(\"orange\")\n","    )\n","  fig <-\n","    fig %>% add_trace(\n","      y = ~ sim,\n","      name = 'Simulation',\n","      mode = 'lines',\n","      color = I(\"green\")\n","    )\n","  fig <-\n","    fig %>% layout(xaxis = list(type = \"date\", title = \"time\"),\n","                   yaxis = list(title = \"Price\"))\n","  return(list(fig, df, model))\n","  \n","}"]},{"cell_type":"markdown","metadata":{},"source":["Il existe dans R une fonction qui permet de déterminer automatiquement un modèle ARIMA addapté à la série temporelle passée en entrée. Cependant, ce modèle n'est pas nécessairement le meilleur possible, étant donné que tout est fait pour limiter au maximum le temps d'exécution. Nous avons donc codé une méthode nommée le grid-search qui va parcourir de nombreux modèle ARIMA possible et séléctionner celui ayant le critère AIC (Akaike Information Criterion, un critère très souvent utilisé pour déterminer la qualité d'un modèle ARIMA) le plus bas. Cependant, cette méthode est vraiment très longue à exécuter (plus d'une heure en fonction des paramètres), c'est pourquoi nous ne l'exécuterons pas dans ce notebook, nous utiliserons directement les résultats obtenus en la lançant sur nos ordinateurs personnels. Tout d'abord, on sépare les séries temporelles en séries d'entraînement et en séries de test. "]},{"cell_type":"code","execution_count":null,"metadata":{"trusted":true},"outputs":[],"source":["btc_ts = ts(btc$Close)\n","split_btc <- ts_split(ts.obj = btc_ts, sample.out = 365)\n","\n","training <- split_btc$train\n","testing <- split_btc$test\n","\n","eth_ts = ts(eth$Close)\n","split_eth <- ts_split(ts.obj = eth_ts, sample.out = 365)\n","\n","training_eth <- split_eth$train\n","testing_eth <- split_eth$test\n","\n","ada_ts = ts(ada$Close)\n","split_ada <- ts_split(ts.obj = ada_ts, sample.out = 365)\n","\n","training_ada <- split_ada$train\n","testing_ada <- split_ada$test\n","\n","xrp_ts = ts(xrp$Close)\n","split_xrp <- ts_split(ts.obj = xrp_ts, sample.out = 365)\n","\n","training_xrp <- split_xrp$train\n","testing_xrp <- split_xrp$test"]},{"cell_type":"markdown","metadata":{},"source":["Attention, la cellule suivante étant extrêmement longue à exécuter, nous l'avons commenté. "]},{"cell_type":"code","execution_count":null,"metadata":{"trusted":true},"outputs":[],"source":["# #Grid-Search pour le Bitcoin\n","# order_list = list(seq(0,14),\n","#                   ndiffs(training),\n","#                   seq(0,14)) %>%\n","#   cross() %>%\n","#   map(lift(c))\n","# orderdf = tibble(\"order\" = order_list)\n","# models_df = orderdf %>%\n","#   mutate(models = map(order, ~possibly(arima, otherwise = NULL)(x = training, order = .x))) %>% \n","#   filter(models != 'NULL') %>%\n","#   mutate(aic = map_dbl(models, \"aic\"))\n","  \n","#   best_model = models_df %>%\n","#     filter(aic == min(models_df$aic, na.rm = TRUE))\n","\n","# best_model\n","# best_model$order\n","# best_model$aic\n","\n","# #Grid-Search pour l'Ethereum\n","# order_list = list(seq(0,14),\n","#                   ndiffs(training_eth),\n","#                   seq(0,14)) %>%\n","#   cross() %>%\n","#   map(lift(c))\n","# orderdf = tibble(\"order\" = order_list)\n","# models_df_eth = orderdf %>%\n","#   mutate(models = map(order, ~possibly(arima, otherwise = NULL)(x = training_eth, order = .x))) %>% \n","#   filter(models != 'NULL') %>%\n","#   mutate(aic = map_dbl(models, \"aic\"))\n","\n","# best_model_eth = models_df_eth %>%\n","#   filter(aic == min(models_df_eth$aic, na.rm = TRUE))\n","\n","# best_model_eth\n","# best_model_eth$order\n","# best_model_eth$aic\n","\n","# #Grid-Search pour le Cardano \n","# order_list = list(seq(0,14),\n","#                   ndiffs(training_ada),\n","#                   seq(0,14)) %>%\n","#   cross() %>%\n","#   map(lift(c))\n","# orderdf = tibble(\"order\" = order_list)\n","# models_df_ada = orderdf %>%\n","